{
  "name": "gi-client-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='-r newrelic' next dev",
    "test": "TZ=UTC jest --silent",
    "test:watch": "TZ=UTC jest --watch",
    "test:cov": "TZ=UTC jest --coverage --collectCoverageFrom='**/*.{tsx,jsx,js,ts}' --passWithNoTests",
    "test:cov:changed": "TZ=UTC jest --coverage --changedSince=origin/develop --collectCoverageFrom='**/*.{tsx,jsx,js,ts}' --passWithNoTests",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "build": "next build",
    "start": "sh ./nextdev.sh && next start",
    "lint": "next lint",
    "prettier": "prettier --single-quote --write \"**/*.{ts,tsx,js,jsx,json,scss,css}\"",
    "staged-prettier": "prettier --write $(git diff --name-only --diff-filter d | grep -e '\\.[tj]sx\\?$' | xargs)",
    "dev-build": "rm -rf .next && next build",
    "safari:setup": "chmod +x ./safari-setup.sh && ./safari-setup.sh",
    "safari:dev": "next dev -p 3100 & local-ssl-proxy --key ./bs-local.com-key.pem --cert ./bs-local.com.pem --source 443 --target 3100",
    "lint:style": "stylelint",
    "lint:style:all": "yarn lint:style '**/*.css'",
    "start-inspect": "NODE_OPTIONS='--inspect' next start"
  },
  "dependencies": {
    "@dotlottie/react-player": "1.6.1",
